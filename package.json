  },
  "devDependencies": {
    "@0x/sol-coverage": "^3.0.3",
    "@0x/sol-profiler": "^3.1.3",
    "@0x/sol-trace": "^2.0.9",
    "@0x/subproviders": "^4.0.5",
    "@soldoc/soldoc": "^0.4.3",
    "istanbul": "^0.4.5",
