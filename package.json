{
  "name": "nocust-contracts-solidity",
  "version": "0.8.0",
  "description": "- Compiler: solc 0.4.24+commit.e67f0147.Emscripten.clang\r - NOCUST paper: https://eprint.iacr.org/2018/642.pdf\r - Technical specifications: http://specs.liquidity.network/\r - Website: http://liquidity.network/",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf coverage",
    "compile": "truffle compile",
    "console": "truffle console",
    "coverage": "npm run clean && MODE=coverage truffle test; istanbul report html",
    "documentation": "soldoc --in contracts -o docs",
    "lint": "solhint --config .solhint.json contracts/*.sol contracts/**/*.sol",
    "test": "truffle test",
    "trace": "MODE=trace truffle test"
  },
